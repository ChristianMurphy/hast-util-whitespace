{
  "name": "hast-util-whitespace",
  "version": "1.0.0",
  "description": "Check whether a node is inter-element whitespace",
  "license": "MIT",
  "keywords": [
    "hast",
    "html",
    "inter",
    "element",
    "inter-element",
    "white-space",
    "whitespace"
  ],
  "dependencies": {},
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/hast-util-whitespace.git"
  },
  "bugs": "https://github.com/wooorm/hast-util-whitespace/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "devDependencies": {
    "browserify": "^13.0.0",
    "esmangle": "^1.0.1",
    "nyc": "^10.0.0",
    "remark-cli": "^2.1.0",
    "remark-preset-wooorm": "^1.0.0",
    "tape": "^4.4.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail --output",
    "build-bundle": "browserify index.js --bare -s hastUtilWhitespace > hast-util-whitespace.js",
    "build-mangle": "esmangle hast-util-whitespace.js > hast-util-whitespace.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test.js",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "xo": {
    "space": true,
    "ignores": [
      "hast-util-is-element.js"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "remarkConfig": {
    "presets": "wooorm"
  }
}
